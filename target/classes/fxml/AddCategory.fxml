<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="-Infinity" prefHeight="495.0" prefWidth="858.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.moja.biblioteka.Controllers.CategoryController">
   <children>
      <HBox alignment="TOP_CENTER" prefHeight="28.0" prefWidth="858.0" spacing="20.0" VBox.vgrow="ALWAYS">
         <children>
            <Label prefHeight="17.0" prefWidth="109.0" text="%add.categorry" />
            <TextField fx:id="categoryTextField" prefHeight="25.0" prefWidth="133.0" />
            <Button fx:id="addCategory" mnemonicParsing="false" onAction="#AddCategoryAction" prefHeight="25.0" prefWidth="71.0" text="%add.button" />
            <Label text="%choose.cattegory" />
            <ComboBox fx:id="comboBox" onAction="#OnActionComboBox" prefHeight="25.0" prefWidth="122.0" />
            <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteAction" text="%delete" />
            <Button fx:id="ControllerEdditButton" mnemonicParsing="false" onAction="#EditControlerAction" prefHeight="25.0" prefWidth="56.0" text="%edit" />
         </children>
         <padding>
            <Insets bottom="15.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </HBox>
      <TreeView fx:id="treeViewControler" maxHeight="1.7976931348623157E308" showRoot="false" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </TreeView>
   </children>
</VBox>
